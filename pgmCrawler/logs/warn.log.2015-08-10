	//滚动事件，判断滚动条的位置
	$(window).scroll(function() {
		//如果滚动条超过浏览器窗口高度
        if( $(this).scrollTop()>0){
			$('#upbtn').fadeIn("slow");                       //显示
			if( $('#school_float_banner').is(":hidden")){
				$('#school_float_banner').fadeOut("slow");	
			}
		} else {
			$('#upbtn').fadeOut("slow");                      //隐藏	
		}
    });
	
	//单击事件，触发回到顶部
	$('#upbtn').click(function(e) {
		$('body,html').animate({scrollTop:0},1000);                
	});	 
	
	$('#areat').click(function(e) {
		$('body,html').animate({scrollTop:0},1000);                
	});	 
	//大学底部登陆关闭
	$("#school_float_banner").find(".close").click(function(e) {
		$(this).parents("#school_float_banner").fadeOut();		
	});
});


