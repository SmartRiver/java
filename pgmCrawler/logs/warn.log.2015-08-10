<script>

function diy_animate() {
	var count = $("#rlscrolldiy ul li").length();
	var index = 0, height=0;
	var dl = $("#rlscrolldiy ul").first();
	var html = $("#rlscrolldiy ul").html();
	$("#rlscrolldiy ul").html(html+html);
	
	var animate = function(stepCount,isadd) {
		if(dl.animateIsLoading) return;
		if(isadd) {
			index += stepCount; height = 24*stepCount*-1;
		}
		if(!isadd) {
			index -=stepCount; height = stepCount*24;
		}
		$(dl).animate({marginTop:"+"+height+"px"}, "fast",function() {
			
			if(index == count) {
				$(dl).css({marginTop:"0px"});
				index = 0;
				
			}
		});
	};
	setInterval(animate.bind(this,1,true), 4000);
	
}
diy_animate();

</script>
